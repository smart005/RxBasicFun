<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_eeeff0"
    android:orientation="vertical">

    <include
        android:id="@+id/head_layout_rl"
        layout="@layout/head_generate_view"></include>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white_color"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="20dp"
        android:text="@string/please_setting_network"
        android:textColor="@color/wireless_prompt_subject_color"
        android:textSize="@dimen/font_size_13" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:background="@color/white_color"
        android:paddingBottom="12dp"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="10dp"
        android:text="@string/please_setting_network_des"
        android:textColor="@color/wireless_prompt_des_color"
        android:textSize="@dimen/font_size_12" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white_color"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="12dp"
        android:text="@string/if_connected_wireless"
        android:textColor="@color/wireless_prompt_subject_color"
        android:textSize="@dimen/font_size_13" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white_color"
        android:paddingBottom="12dp"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="10dp"
        android:text="@string/if_connected_wireless_des"
        android:textColor="@color/wireless_prompt_des_color"
        android:textSize="@dimen/font_size_12" />

    <TextView
        android:id="@+id/channel_source_tv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white_color"
        android:gravity="bottom|center_horizontal"
        android:paddingBottom="8dp"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:textColor="@color/wireless_prompt_des_color"
        android:textSize="@dimen/font_size_11" />

</LinearLayout>